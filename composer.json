{
  "name": "icanhazstring/phpstan-readonly-property",
  "type": "library",
  "description": "Support #[IsReadonly] promoted constructor properties for PHPStan",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Fr√∂mer",
      "email": "andreas.froemer@check24.de"
    }
  ],
  "require": {
    "php": "^8.0",
    "phpstan/phpstan": "^0.12.55 || ^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Icanhazstring\\PhpstanReadonlyPropertyExtension\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Icanhazstring\\PhpstanReadonlyPropertyExtension\\Test\\": "tests/"
    },
    "classmap": [
      "tests/data"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "phpstan": {
      "includes": [
        "rules.neon"
      ]
    }
  }
}
